<?xml version="1.0" encoding="utf-8"?> 
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <RootDirectory Condition="'$(RootDirectory)' == ''">$(MSBuildThisFileDirectory)</RootDirectory>
    <BuildDirectory Condition="'$(BuildDirectory)' == ''">$(SolutionDir)</BuildDirectory>
    <ConfigDirectory Condition="'$(ConfigDirectory)' == ''">$(SolutionDir)Config\</ConfigDirectory>
    <BuiltDirectory Condition="'$(BuiltDirectory)' == ''">$(RootDirectory)Built\</BuiltDirectory>
    <SourceDirectory Condition="'$(SourceDirectory)' == ''">$(RootDirectory)Source\</SourceDirectory>
    <ThirdPartyDirectory Condition="'$(ThirdPartyDirectory)' == ''">$(RootDirectory)Third Party\</ThirdPartyDirectory>
    <PackagesDirectory Condition="'$(PackagesDirectory)' == ''">$(RootDirectory)Packages\</PackagesDirectory>
  </PropertyGroup>
</Project>